{
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.fixAll.eslint": "explicit"
    },
    "editor.formatOnSave": true,
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "eslint.alwaysShowStatus": true,
    "postman.settings.dotenv-detection-notification-visibility": false
}